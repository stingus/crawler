{
  "name": "stingus/crawler",
  "type": "project",
  "description": "Crawler for exchange rates and weather",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Kerpician",
      "email": "arthur@bluechip.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Stingus\\Crawler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stingus\\Crawler\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6",
    "ext-dom": "*",
    "symfony/dom-crawler": "^3",
    "symfony/config": "^3",
    "symfony/yaml": "^3",
    "symfony/console": "^3",
    "guzzlehttp/guzzle": "^6",
    "swiftmailer/swiftmailer": "^5"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^5",
    "squizlabs/php_codesniffer":   "^2",
    "codeclimate/php-test-reporter": "^0"
  }
}
